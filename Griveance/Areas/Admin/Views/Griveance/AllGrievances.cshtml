
@{
    ViewBag.Title = "AllGrievances";
    Layout = "~/Areas/Admin/Views/Shared/AdminDashboardLayout.cshtml";
}

<h2>All Posted Grievances</h2>
<html lang="en" class="no-js">

<body ng-app="GR">
    @using System.Configuration;
    <div ng-controller="GrievanceController">
        <form class="student-admission-wrapper" ng-init="Initialize()">
            <table class="table table-striped- table-bordered table-hover table-checkable" id="m_table_1">
                <thead>
                    <tr>
                        <th>Complaint Number</th>
                        <th>Grievance Type</th>
                        <th>Date</th>
                        <th>Grievance Cell Member</th>
                        <th>Designation</th>
                        <th>Subject</th>
                        <th>Post</th>
                        <th>Grievances Action</th>
                        <th>Status</th>

                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="data in GrievanceList">
                        <td>
                            {{data.CompID}}
                        </td>
                        <td>
                            {{data.GrievanceType}}
                        </td>
                        <td>
                            {{data.Date}}
                        </td>
                        <td>
                            {{data.name}}
                        </td>
                        <td>
                            {{data.Subject}}
                        </td>
                        <td>
                            {{data.Post}}
                        </td>
                        <td>
                            {{data.GrievanceAction}}
                        </td>
                        <td>
                            {{data.status}}
                        </td>
                        <td>
                            {{data.status}}
                        </td>

                    </tr>

                </tbody>
            </table>
        </form>
    </div>
    

    <script src="@Url.Content("~/Content/angularjs/scripts/angular.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ProjectScript/Grievance/GrievanceTypeModule.js")"></script>
    <script src="@Url.Content("~/Scripts/Common/Service.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ProjectScript/Grievance/GrievanceTypeList.js")" type="text/javascript"></script>


    <script>
        var baseURL = "@ConfigurationManager.AppSettings["BaseUrlPath"].ToString()";
    </script>
</body>
</html>

