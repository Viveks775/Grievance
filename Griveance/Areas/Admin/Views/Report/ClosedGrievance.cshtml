
@{
    Layout = "~/Areas/Admin/Views/Shared/ReportLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Closed Grievance</title>
</head>
<body>
    <div id="Grid">
       
    <h2 align="center">Grievance Redressal Portal</h2>
    <hr />
    <h4 align="center">Closed Grievance Report</h4>
    <hr />

    <div class="col-xl-auto">

        <!--begin::Portlet-->
        <div class="m-portlet m-portlet--last m-portlet--head-lg m-portlet--responsive-mobile" id="main_portlet">

            <div class="m-portlet m-portlet--mobile">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <b>From Date-</b>&nbsp; @ViewData["Fromdate"] &nbsp;&nbsp;
                            <b>To Date-</b>&nbsp; @ViewData["Todate"]
                        </div>
                    </div>

                </div>

            </div>
            <table align="center" class="table table-striped- table-bordered table-hover table-checkable" border="1">
                <tr  align="center">
                    <th>Sr.No</th>
                    <th>Comp.Id</th>
                    <th>Complainants Type</th>
                    <th>Complainants Name</th>
                    <th>Grievance Type</th>
                    <th>Date</th>
                    <th>Subject</th>
                </tr>
                <tr  align="center">
                    <td>1</td>
                    <td>2</td>
                    <td>No</td>
                    <td>No</td>
                    <td>No</td>
                    <td>1-1-2018</td>
                    <td>Subject</td>
                </tr>
                <tr  align="center">
                    <td>2</td>
                    <td>3</td>
                    <td>No</td>
                    <td>No</td>
                    <td>No</td>
                    <td>1-1-2018</td>
                    <td>Subject</td>
                </tr>
                <tr  align="center">
                    <td>3</td>
                    <td>4</td>
                    <td>No</td>
                    <td>No</td>
                    <td>No</td>
                    <td>1-1-2018</td>
                    <td>Subject</td>
                </tr>
            </table>
        </div>
    </div>

</div>
</body>
</html>
