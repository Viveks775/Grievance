
@{
    ViewBag.Title = "FacultyGrievance";
    Layout = "~/Areas/Faculty/Views/Shared/FacultyDashboard.cshtml";
}

<h2>Faculty Grievance</h2>
<html lang="en" class="no-js">

<body ng-app="GR">
    @using System.Configuration;
    <div ng-controller="UsersController">
        <form class="student-admission-wrapper" ng-init="GetData()">
            <table class="table table-striped- table-bordered table-hover table-checkable" id="m_table_1">
                <thead>
                    <tr>
                        <th>Complaint Number</th>
                        <th>Grievance Type</th>
                        <th>Date</th>
                        <th>Subject</th>
                        <th>Post</th>
                        <th>Grievances Action</th>
                        <th>Status</th>

                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="data in Student">
                        <td>
                            {{data.CompID}}
                        </td>
                        <td>
                            {{data.GrievanceType}}
                        </td>
                        <td>
                            {{data.Date}}
                        </td>
                        <td>
                            {{data.Subject}}
                        </td>
                        <td>
                            {{data.Post}}
                        </td>
                        <td>
                            {{data.GrievanceAction}}
                        </td>
                        <td>
                            {{data.status}}
                        </td>


                    </tr>
                </tbody>
            </table>




        </form>
    </div>

    <script src="@Url.Content("~/Content/angularjs/scripts/angular.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ProjectScript/Faculty/FacultyModule.js")"></script>
    <script src="@Url.Content("~/Scripts/Common/Service.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ProjectScript/Faculty/FacultyController.js")" type="text/javascript"></script>


    <script>
        var baseURL = "@ConfigurationManager.AppSettings["BaseUrlPath"].ToString()";
    </script>
</body>
</html>


